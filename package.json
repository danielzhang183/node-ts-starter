{
  "name": "pkg-name",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.15.5",
  "description": "A node starter template with using ts",
  "author": "Daniel Zhang",
  "license": "MIT",
  "homepage": "https://github.com/danielzhang183/pkg-name#readme",
  "repository": "danielzhang183/pkg-name",
  "bugs": "https://github.com/danielzhang183/pkg-name/issues",
  "keywords": [
    "node",
    "ts",
    "node-ts-starter"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@types/node": "^18.16.6",
    "bumpp": "^8.2.1",
    "eslint": "^8.40.0",
    "esno": "^0.16.3",
    "lint-staged": "^15.2.2",
    "pnpm": "^8.5.0",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^4.9.5",
    "unbuild": "^0.8.11",
    "vite": "^5.2.7",
    "vitest": "^0.23.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
